{
    "name": "hjp1011/yiiframe",
    "description": "A fast background development framework based on Yii2+Bootstrap",
    "keywords": ["yii2", "framework", "advanced", "project template", "rageframe", "rageframe2"],
    "homepage": "http://www.yiiframework.com/",
    "type": "project",
    "license": "Apache-2.0",
    "support": {
        "issues": "https://github.com/yiisoft/yii2/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii2"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.1.0",
        "yiisoft/yii2": "~2.0.14",
        "yiisoft/yii2-swiftmailer": "~2.0.0 || ~2.1.0",
        "yiisoft/yii2-imagine": "~2.0.0",
        "yiisoft/yii2-elasticsearch": "2.1.x-dev",
        "yiisoft/yii2-authclient": "~2.1.0",
        "yiisoft/yii2-redis": "^2.0",
        "yiisoft/yii2-queue": "dev-master",
        "kartik-v/yii2-widget-datepicker": "@dev",
        "kartik-v/yii2-widget-timepicker": "*",
        "kartik-v/yii2-widget-datetimepicker": "dev-master",
        "kartik-v/yii2-widget-colorinput": "*",
        "kartik-v/yii2-widget-select2": "@dev",
        "kartik-v/yii2-date-range": "*",
        "hjp1011/yii2-treegrid": "^1.0",
        "hjp1011/yii2-console-migration": "^1.0",
        "hjp1011/yii2-easy-wechat": "^1.0",
        "hjp1011/yii2-gii": "^1.0.0",
        "hjp1011/yii2-plugs": "^1.0.0",
        "hjp1011/yii2-adminlte": "^1.0.0",
        "omnipay/common": "^3.0",
        "lokielse/omnipay-alipay": "^3.0",
        "lokielse/omnipay-wechatpay": "^3.0",
        "lokielse/omnipay-unionpay": "^3.0",
        "2amigos/qrcode-library": "^1.1",
        "aliyuncs/oss-sdk-php": "~2.0",
        "linslin/yii2-curl": "*",
        "overtrue/pinyin": "~3.0",
        "overtrue/easy-sms": "^1.0",
        "overtrue/flysystem-cos": "^2.0",
        "overtrue/wechat": "~4.2.2",
        "overtrue/flysystem-qiniu": "^1.0",
        "league/flysystem": "^1.0",
        "xxtime/flysystem-aliyun-oss": "^1.2",
        "xj/yii2-oauth": "~2.0.0",
        "ramsey/uuid": "^3.7",
        "moontoast/math": "^1.1",
        "unclead/yii2-multiple-input": "~2.0",
        "zhuzhichao/ip-location-zh": "^2.4",
        "phpoffice/phpspreadsheet": "^1.3",
        "jpush/jpush": "*",
        "omnilight/yii2-scheduling": "*",
        "hashids/hashids": "^3.0",
        "league/oauth2-server": "^7.4",
        "mobiledetect/mobiledetectlib": "^2.8",
        "jaeger/querylist": "^4.1",
        "mingyoung/dingtalk": "2.0",
        "nelexa/zip": "^3.3",
        "finecho/logistics": "^3.1.1",
        "tencentcloud/tencentcloud-sdk-php": "3.0.94",
        "ext-bcmath": "*",
        "fastgoo/getui-php": "^1.0",
        "dueros/bot-sdk": "^2.2",
        "griffinledingham/php-apple-signin": "^1.1",
        "firebase/php-jwt": "^5.2",
        "yly-openapi/yly-openapi-sdk": "v1.0.1",
        "league/omnipay": "^3.0",
        "omnipay/stripe": "^3.1"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.0.0",
        "yiisoft/yii2-gii": "~2.0.0",
        "yiisoft/yii2-faker": "~2.0.0",
        "codeception/base": "^2.4.0",
        "phpunit/phpunit": "~6.5.5",
        "codeception/verify": "~1.0.0",
        "swoole/ide-helper": "@dev"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "easywechat-composer/easywechat-composer": true,
            "php-http/discovery": true
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer/"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
